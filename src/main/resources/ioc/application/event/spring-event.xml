<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


    <bean name="customizeListener" class="com.spring.ioc.appliction.event.listener.CustomizeApplicationListener"></bean>

    <bean name="customizeGenericApplicationListener" class="com.spring.ioc.appliction.event.listener.CustomizeGenericApplicationListener"></bean>

    <bean name="stringEventListener" class="com.spring.ioc.appliction.event.listener.StringEventListener"></bean>

    <bean name="helloWoldListener" class="com.spring.ioc.appliction.event.listener.WildcardEventListener"></bean>

    <bean name="longEventListener" class="com.spring.ioc.appliction.event.listener.LongEventListener"></bean>
</beans>